{
  "name": "kodokojo-ui",
  "version": "0.1.0",
  "description": "Kodo Kojo UI",
  "main": "src/scripts/app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kodokojo/kodokojo-ui.git"
  },
  "scripts": {
    "dependencies": "ncu",
    "dependencies:check": "npm-check",
    "dependencies:update": "npm-check -u -E",
    "clean": "test -d ./node_modules/ && rm -rf ./node_modules || echo \"no node_modules/ folder, continue\"",
    "clean:static": "test -d ./static/ && rm -rf ./static || echo \"no static/ folder, continue\"",
    "styleguide": "start-storybook -p 9001 -c ./styleguide",
    "license": "gulp license",
    "build": "npm run build:env && npm run build:index",
    "build:env": "node ./config/config.output.js | mustache - config/shared/api.env.mustache > config/shared/api.env.js && npm run version",
    "build:index": "mustache webpack-assets.json src/index.mustache > static/index.html",
    "build:styleguide": "build-storybook -c ./styleguide -o styleguide/dist",
    "prebuild:dev": "NODE_ENV=development BABEL_ENV=development npm run build:env",
    "build:dev": "NODE_ENV=development BABEL_ENV=development webpack --progress --colors && npm run build:index",
    "start:dev": "npm run build:dev && NODE_ENV=development BABEL_ENV=development node server.js",
    "prebuild:custom": "NODE_ENV=custom BABEL_ENV=development npm run build:env",
    "build:custom": "NODE_ENV=custom BABEL_ENV=development webpack --progress --colors && npm run build:index",
    "start:custom": "npm run build:custom && NODE_ENV=custom BABEL_ENV=development node server.js",
    "prebuild:prod": "npm run clean:static && NODE_ENV=production BABEL_ENV=production npm run build:env",
    "build:prod": "NODE_ENV=production BABEL_ENV=production webpack -p --progress --colors && npm run build:index",
    "start:prod": "npm run build:prod && NODE_ENV=production BABEL_ENV=production node server.js",
    "prebuild:local": "npm run clean:static && NODE_ENV=local BABEL_ENV=production npm run build:env",
    "build:local": "NODE_ENV=local BABEL_ENV=production webpack -p --progress --colors && npm run build:index",
    "start:local": "npm run build:local && NODE_ENV=local BABEL_ENV=production node server.js",
    "prebuild:mock": "NODE_ENV=mock BABEL_ENV=development npm run build:env",
    "build:mock": "webpack --progress --colors && npm run build:index",
    "start:mock": "npm run build:mock && NODE_ENV=mock BABEL_ENV=development node server.js",
    "start:server:mock": "node server.mock.js",
    "build:docker": "./build.sh",
    "lint": "eslint ./src/ ./api/ ./config/ --ext .js --ext .jsx",
    "pretest": "NODE_ENV=development BABEL_ENV=test npm run build:env",
    "test": "BABEL_ENV=test mocha --compilers js:./test/compiler.js --recursive --require ignore-styles --require intl --require ./test/utils.js -R dot './src/**/*.spec.js'",
    "test:watch": "npm test -- --watch",
    "coverage": "BABEL_ENV=test babel-node node_modules/isparta/bin/isparta cover _mocha --root src/ --include-all-sources --default-excludes -- --recursive './src/**/*.spec.js' --require ignore-styles --require intl --require ./test/utils.js",
    "version": "node ./config/ui.version.js"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "author": {
    "name": "ModuloM / Antoine Le Taxin",
    "url": "https://twitter.com/ModuloM"
  },
  "contributors": [
    {
      "name": "Antoine LE TAXIN",
      "url": "https://twitter.com/ModuloM"
    },
    {
      "name": "Jean-Pascal THIERY",
      "url": "https://twitter.com/jpthiery"
    }
  ],
  "license": "GPL-3.0",
  "engines": {
    "node": "5.x.x",
    "npm": "3.x.x"
  },
  "devDependencies": {
    "@kadira/storybook": "1.29.0",
    "assets-webpack-plugin": "3.4.0",
    "babel-cli": "6.6.5",
    "babel-core": "6.5.2",
    "babel-eslint": "6.0.2",
    "babel-istanbul": "0.8.0",
    "babel-loader": "6.2.3",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-rewire": "1.0.0-rc-1",
    "babel-plugin-syntax-jsx": "6.5.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.5.2",
    "babel-plugin-transform-object-rest-spread": "6.5.0",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-es2015-loose": "7.0.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.3.13",
    "babel-template": "6.7.0",
    "babel-types": "6.7.2",
    "body-parser": "1.15.0",
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "chai-enzyme": "0.4.1",
    "compression-webpack-plugin": "0.3.1",
    "css-loader": "0.23.1",
    "enzyme": "2.2.0",
    "eslint": "2.11.1",
    "eslint-config-airbnb-base": "3.0.1",
    "eslint-loader": "1.3.0",
    "eslint-plugin-babel": "3.1.0",
    "eslint-plugin-import": "1.8.1",
    "eslint-plugin-react": "5.0.1",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "gulp": "3.9.1",
    "gulp-changed": "1.3.2",
    "gulp-header-license": "1.0.6",
    "ignore-styles": "1.2.0",
    "intl": "1.2.4",
    "isparta": "4.0.0",
    "jsdom": "8.1.0",
    "kodokojo-mocks": "0.2.0",
    "json-loader": "^0.5.4",
    "less": "2.6.1",
    "less-loader": "2.2.3",
    "mocha": "2.5.3",
    "mustache": "2.2.1",
    "nock": "8.0.0",
    "node-sass": "3.4.2",
    "npm-check": "5.2.3",
    "npm-check-updates": "2.6.5",
    "pre-commit": "1.1.3",
    "react-addons-test-utils": "15.0.1",
    "react-transform-hmr": "1.0.4",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.0",
    "redux-devtools-log-monitor": "1.0.11",
    "redux-mock-store": "1.1.0",
    "request-promise": "3.0.0",
    "resolve-url": "0.2.1",
    "resolve-url-loader": "1.4.3",
    "sass-loader": "3.2.0",
    "sinon": "1.17.3",
    "sinon-as-promised": "4.0.0",
    "sinon-chai": "2.8.0",
