    "winston": "2.2.0"
  },
  "dependencies": {
    "babel-polyfill": "6.9.0",
    "better-console": "0.2.4",
    "bluebird": "3.4.0",
    "classnames": "2.2.5",
    "del": "2.2.0",
    "eventsource-polyfill": "0.9.6",
    "extend": "3.0.0",
    "import": "0.0.6",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.13.1",
    "map-stream": "0.0.6",
    "normalize-css": "2.3.1",
    "normalize.css": "4.1.1",
    "normalizr": "2.1.0",
    "react": "15.3.1",
    "react-addons-css-transition-group": "15.1.0",
    "react-css-themr": "1.1.0",
    "react-dom": "15.1.0",
    "react-intl": "2.1.2",
    "react-redux": "4.4.5",
    "react-router": "2.4.1",
